{
    "builds": [
      {
        "src": "api/index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "dist/public/**/*",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/public/$1"
      }
    ]
  }
  