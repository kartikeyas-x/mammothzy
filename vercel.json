
{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    { "source": "/api/healthcheck", "destination": "/vercel/api/healthcheck" },
    { "source": "/api/debug-db", "destination": "/vercel/api/debug-db" },
    { "source": "/api/debug", "destination": "/vercel/api/debug" },
    { "source": "/api/detailed-debug", "destination": "/vercel/api/detailed-debug" },
    { "source": "/api/(.*)", "destination": "/vercel/api" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
